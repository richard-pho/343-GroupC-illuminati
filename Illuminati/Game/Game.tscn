script/source = "extends Control
var deck
var yuppies
var rollA
var rollB

func _ready():
	deck = Deck.Deck.new()
	yuppies = Yuppies.Yuppies.new()

func _on_Button_pressed():
	get_tree().change_scene(\"res://HomeScreen.tscn\")



func _on_Button2_pressed():
	deck.addCard(yuppies)
	deck.printDeck()


func _on_Dice_pressed():

	rollA = randi()%7+1
	rollB = randi()%7+1
	
	get_node(\"Output\").text=str(rollA+rollB)
"


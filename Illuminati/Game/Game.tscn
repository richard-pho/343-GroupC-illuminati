func _on_DeckDraw_pressed():
	var topCard = deck.deck[0]
	print(topCard.type)
	if(topCard.type == \"group\"): # checking if the card drawn was a group card
		global.uncontrolledGroups.append(topCard)
		# check if all 4 uncontrolled groups in the beginning of game are there
		if(global.uncontrolledGroups.size() == 4): 
			beginning = false
		var path = \"res://Card Assets Folder/\" + topCard.getName() + \".JPG\"
		var image = load(path)
		AAA.texture = image
		var scriptpath = \"res://Card Assets Folder/\" + topCard.getName() + \".gd\"
		var script = load(scriptpath)
		AAA.set_script(script)
		deck.removeCard(topCard)
	else: # then the card drawn was a special card
		if beginning: # if we are still trying to get 4 uncontrolled groups, move special card to bottom
			deck.removeCard(topCard)
			deck.addCard(topCard)
		else: # player draws the special card
			deck.removeCard(topCard)

func _on_Button_pressed():
	get_tree().change_scene(\"res://HomeScreen.tscn\")

func max_arr(arr):
	var max_val = arr[0]

	for i in range(1, arr.size()):
		max_val = max(max_val, arr[i])

	return max_val

# determine turn order
func _on_Dice_pressed():
	# roll dices
	rollA = randi()%6+1
	rollB = randi()%6+1
	var sum = rollA + rollB 	# calculate sum of the dice rolls
	get_node(\"Output\").text=str(sum)
	order.append(sum)			# append the totals to the order array
	# Check if amount of rolls reached number of players
	if (order.size() == global.players.size()):
		# Highest value in the array
		h_val = max_arr(order)
		# Find index of highest value
		index = order.find(h_val, 0)
		var length = order.size()
		var i = 0
		# Run through the array until it prints all the names in the turn order
		while i < order.size():
			global.turn.append(global.players[(i+index)%length].userName)
			print(global.players[(i+index)%length].userName)
			i+=1


